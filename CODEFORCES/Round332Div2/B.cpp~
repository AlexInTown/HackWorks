#include <iostream>
#include <memory.h>
using namespace std;
int n, m;
int count[100001];
int findex[100001];
int a[100001];
int main(){
cin>>n>>m;
memset(count, 0, sizeof(n+1));
memset(findex, 0, sizeof(n+1));
memset(a, 0, sizeof(m+1));
int i,val;
bool ambi = false;
bool poss = true;
for(i = 1; i <= m; ++i){
    cin>>val;
    count[val] ++;
    if(count[val] > 1) ambi = true;
    findex[val] = i;
}
for(i = 1; i <= m; ++i){
    cin>>val;
    count[val] --;
    if(count[val] < 0) poss = false;
    a[i] = findex[val];
}
if(!poss){
cout<<"Impossible"<<endl;
}else if (ambi){
cout<<"Ambiguity"<<endl;
}else{
cout<<"Possible"<<endl;
    for(i=1; i <= m; ++i){
        cout<<a[i]<<" ";
    }
    cout<<endl;
}
return 0;
}
